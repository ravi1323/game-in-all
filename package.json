{
  "name": "gameinall",
  "version": "1.0.0",
  "description": "package for all simple multiplayer game",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare" : "npm run build",
    "prepublishOnly" : "npm test && npm run lint",
    "preversion" : "npm run lint",
    "version" : "npm run format && git add -A src",
    "postversion" : "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ravi1323/game-in-all.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "socket.io",
    "redis",
    "mongodb"
  ],
  "author": "ravivora",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ravi1323/game-in-all/issues"
  },
  "homepage": "https://github.com/ravi1323/game-in-all#readme",
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.0.4"
  },
  "files": [
    "lib/**/*"
  ]
}
